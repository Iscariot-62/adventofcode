#include <stdio.h>
#include <stdlib.h>
#include <string.h>

//Läuft das Muster nach der vorgegebenen Regel ab, und fängt wieder vorne an, wenn er hinten "rausfällt"
void walk(int *pos, int values){
int temp=*pos;
temp+=3;
if(temp<=values){
    *pos = temp;
}
else{
*pos = temp-values;
}
}

